{
    "name": "esch/oxiddemomodule",
    "description": "Demo Module For Marmalade",
    "type": "oxideshop-module",
    "extra": {
        "oxideshop": {
            "blacklist-filter": [
                "documentation/**/*.*"
            ],
            "target-directory": "esch/oxiddemomodule"
        }
    },
    "require": {
        "php": ">=5.6",
        "lib-curl": ">=7.26.0",
        "lib-openssl": ">=1.0.1",
        "ext-curl": "*",
        "ext-openssl": "*",
        "eberhardschneider/oxiddemoodule": "master"
    },
    "autoload": {
        "psr-4": {
            "ESchneider\\OxidDemoModule\\": "../../../source/modules/esch/oxiddemomodule"
        }
    }
}